{
  "tabs": {
    "file_icons": true,
    "git_status": false
  },
  "completions": {
    "lsp": true
  },
  "enable_language_server": true,
  "inlay_hints": {
    "enabled": false
  },
  "indent_guides": {
    "background_coloring": "disabled",
    "coloring": "fixed",
    "enabled": true
  },
  "current_line_highlight": "gutter",
  "theme": "Lydia",
  "icon_theme": "Soft Charmed Icons",
  "helix_mode": false,
  "ui_font_size": 15.0,
  "ui_font_family": "JetBrains Mono",
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 16.0,
  "double_click_in_multibuffer": "open",
  "soft_wrap": "bounded",
  "lsp_document_colors": "inlay",
  "show_wrap_guides": false,
  "gutter": {
    "line_numbers": true,
    "breakpoints": false,
    "min_line_number_digits": 3,
    "runnables": false,
    "folds": true
  },
  "git_panel": {
    "button": false,
    "dock": "bottom"
  },
  "terminal": {
    "button": false,
    "dock": "bottom",
    "font_size": 15,
    "font_family": "JetBrains Mono"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "collaboration_panel": {
    "button": false
  },
  "title_bar": {
    "show_menus": false,
    "show_sign_in": true,
    "show_user_picture": true,
    "show_onboarding_banner": true,
    "show_project_items": true,
    "show_branch_icon": true
  },
  "status_bar": {
    "active_language_button": true,
    "cursor_position_button": false
  },
  "debugger": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "diagnostics": {
    "button": false
  },
  "search": {
    "button": false
  },
  "project_panel": {
    "entry_spacing": "comfortable",
    "button": false,
    "hide_root": true,
    "indent_size": 12,
    "sticky_scroll": false,
    "indent_guides": {
      "show": "never"
    }
  },
  "global_lsp_settings": {
    "button": false
  },
  "toolbar": {
    "agent_review": true,
    "breadcrumbs": false,
    "code_actions": false,
    "selections_menu": false,
    "quick_actions": false
  },
  "agent": {
    "button": false,
    "dock": "right",
    "model_parameters": []
  },
  "languages": {
    "JavaScript": {
      "formatter": ["auto", { "code_action": "source.fixAll.eslint" }]
    }
  }
}
